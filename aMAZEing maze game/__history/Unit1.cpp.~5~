//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "Главное_меню.h"
#include "Unit2.h"
#include "Unit3.h"
#include "Unit8.h"

//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TFormMenu *FormMenu;
//---------------------------------------------------------------------------
__fastcall TFormMenu::TFormMenu(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TFormMenu::Button3Click(TObject *Sender)
{
	FormSkins->Show();
	FormMenu->Hide();
}
//---------------------------------------------------------------------------
void __fastcall TFormMenu::Button1Click(TObject *Sender)
{
	FormDificult->Show();
	FormMenu->Hide();
}
//---------------------------------------------------------------------------
void __fastcall TFormMenu::Button4Click(TObject *Sender)
{
    FormMenu->Close();
}
//---------------------------------------------------------------------------

void __fastcall TFormMenu::Button2Click(TObject *Sender)
{
	FormMenu->Hide();
	FormRateing->Show();
}
//---------------------------------------------------------------------------

